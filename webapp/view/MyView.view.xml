<mvc:View xmlns:core="sap.ui.core" controllerName="MyApp.controller.MyController" xmlns:mvc="sap.ui.core.mvc" xmlns:f="sap.ui.layout.form" xmlns="sap.m">
		<Table id="purchaseOrdersTable" items="{/myPOs}">
			<columns>
				<Column>
					<Text text="PO Number" />
				</Column>
				<Column>
					<Text text="Description" />
				</Column>
				<Column>
					<Text text="Total Amount" />
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{poNumber}" /> 
						<Text text="{description}" /> 
						<Text text="{totalAmount}" /> 
					</cells>
				</ColumnListItem>
			</items>
		</Table>
</mvc:View>