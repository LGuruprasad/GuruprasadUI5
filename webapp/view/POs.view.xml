<mvc:View
    xmlns:core="sap.ui.core"
    controllerName="MyApp.controller.POs"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
>
    <VBox>
        <Table>
            <columns>
                <Column width="12em">
                    <Text text="Product" />
                </Column>
                <Column minScreenWidth="Tablet">
                    <Text text="Supplier" />
                </Column>
                <Column>
                    <Text text="Dimensions" />
                </Column>
                <Column>
                    <Text text="Weight" />
                </Column>
                <Column hAlign="End">
                    <Text text="Price" />
                </Column>
            </columns>
            <items />
        </Table>
        <Button
            text="{i18n>goback}"
            press=".goBack"
        />
    </VBox>
</mvc:View>
