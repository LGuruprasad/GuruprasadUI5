{
    "_version": "1.30.0",
    "sap.app": {
        "id": "MyApp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_belize",
            "sap_fiori_3"
        ]
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "MyApp.view.MyView",
            "type": "XML",
            "async": true
        },
        "dependencies": {
            "minUI5Version": "1.60.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "models": {
            "": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/data.json"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "MyApp.i18n.i18n"
                }
            }
        }
    }
}
