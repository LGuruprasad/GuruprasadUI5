<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="sap.btp.sapui5.controller.List" displayBlock="true">
	<Page id="listPage" title="{i18n>ListTitle}">
		<subHeader>
			<Bar id="_IDGenBar1">
				<contentLeft>
					<SearchField id="_IDGenSearchField1" search="handleSearch"/>
				</contentLeft>
			</Bar>
		</subHeader>
  <content>
      <Table noDataText="Drop column list items here and columns in the area above" id="table0" items="{/Products}">
          <items>
              <ColumnListItem type="Active" id="item0">
                  <cells>
                      <Text text="{ProductID}" id="text6"/>
                      <Text text="{ProductName}" id="text7"/>
                      <Text text="{UnitPrice} !" id="text8"/></cells>
              </ColumnListItem>
          </items>
          <columns>
              <Column id="column0">
                  <header>
                      <Label text="{i18n>ProductID}" id="label0"/>
                  </header>
              </Column>
              <Column id="column1">
                  <header>
                      <Label text="{i18n>ProductName}" id="label1"/>
                  </header>
              </Column>
              <Column id="column2">
                  <header>
                      <Label text="{i18n>UnitPrice}" id="label2"/>
                  </header>
              </Column></columns></Table>
  </content>
  </Page>
</mvc:View>